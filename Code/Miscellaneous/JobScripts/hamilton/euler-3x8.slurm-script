#!/bin/csh
#SBATCH -p par6.q
#SBATCH -N 3
#SBATCH --ntasks-per-node=8
#SBATCH --array 1-5
#SBATCH -t 08:00:00
#SBATCH --exclusive
module purge
module load slurm
module load intel/xe_2015.2
module load intel_mpi/5.0.1
module unload gcc/4.9.1
module load gcc/4.9.1

set output=$SLURM_NTASKS"x8-"$SLURM_ARRAY_TASK_ID".results"
mpirun ./ExaHyPE-Euler eulerflow-$SLURM_ARRAY_TASK_ID.exahype > $output

